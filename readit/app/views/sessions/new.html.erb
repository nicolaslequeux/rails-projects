<!-- NOUVELLE VERSION AVEC FORM_WITH -->

<%= form_with url: session_path do |form| %>

  <p>Please log in</p>
  <br>
  <p>
    <label>Email</label>
    <%= form.email_field :email %>
  </p>

  <p>
    <label>Password</label>
    <%= form.password_field :password %>
  </p>

  <p><%= submit_tag 'login' %></p>

<% end %>
